@model IEnumerable<BibliotecaMetrópolis.Tesis>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Titulo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AnnoPublic)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InstitucionEducativa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PalabraBusqueda)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pais.Nombre)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Titulo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AnnoPublic)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.InstitucionEducativa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PalabraBusqueda)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pais.Nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdTesis }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdTesis }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdTesis })
        </td>
    </tr>
}

</table>
